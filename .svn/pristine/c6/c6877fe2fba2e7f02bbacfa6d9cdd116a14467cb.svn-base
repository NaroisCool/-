a,
body {
	color: #333;
}

.btn,
 {
	display: inline-block
}

@font-face {
	font-family: iconfont;
	src: url(//ssl.aomygodstatic.com/member/fonts/passport/iconfont.eot?v=4af33a6d5ded7acf);
	src: url(//ssl.aomygodstatic.com/member/fonts/passport/iconfont.eot?v=4af33a6d5ded7acf#iefix) format('embedded-opentype'), url(//ssl.aomygodstatic.com/member/fonts/passport/iconfont.woff?v=2a850462fedd60d5) format('woff'), url(//ssl.aomygodstatic.com/member/fonts/passport/iconfont.ttf?v=6c36d4db3c345a9e) format('truetype'), url(//ssl.aomygodstatic.com/member/fonts/passport/iconfont.svg?v=c71748e2eed7fcc0#iconfont) format('svg')
}

.iconfont {

	font-family: iconfont!important;
	font-size: 16px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-webkit-text-stroke-width: .2px;
	-moz-osx-font-smoothing: grayscale
}



body {
	font-family: "Microsoft Yahei", "Helvetica Neue", Helvetica, Arial, "Hiragino Sans GB", tahoma, SimSun, sans-serif;
	font-size: 12px;
	background: #fff;
	min-width: 1050px
}

::selection {
	background: #f03468;
	color: #FFF
}

::-moz-selection {
	background: #f03468;
	color: #FFF
}

blockquote,
body,
button,
dd,
dl,
dt,
fieldset,
form,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
input,
legend,
li,
ol,
p,
pre,
td,
textarea,
th,
ul {
	margin: 0;
	padding: 0;
	outline: 0
}

a,
img {
	border: 0;
}

.btn,
.close,

a,
li {
	transition: background .6s, color .6s, border .8s, transform 1s;
	-moz-transition: background .6s, color .6s, border .8s, -moz-transform 1s;
	-webkit-transition: background .6s, color .6s, border .8s, -webkit-transform 1s;
	-o-transition: background .6s, color .6s, border .8s, -o-transform 1s
}

a {
	text-decoration: none
}


a:hover {
	text-decoration: underline;
}
.btn,
.btn-code {
	color: #fff;
	font-family: "微软雅黑";
	cursor: pointer
}

ol,
ul {
	list-style: none
}

.w990 {
	width: 990px;
	margin: 0 auto
}

.box {
	border: 1px solid #e5e5e5;
	padding: 80px 0 60px;
	margin-top: 10px
}

.f-l {
	float: left;
}

.f-r {
	float: right
}

.btn {
	line-height: 38px;
	text-align: center;
	background: darkslateblue;
	border-radius: 2px;
	font-size: 18px;
	border: 0
}

.btn:hover {
	background: #fa4678
}

.btn-disabled,
.btn-disabled:hover {
	background: #f2f2f2
}

.btn-code {
	background: #ffba00;
	width: 130px;
	line-height: 40px;
	height: 40px;
	border: 0;
	margin-left: -5px;
	font-size: 14px;
	position: absolute
}


.footer,
.head .login .ft,
.middle .login-panel .ft .title,
.txt-remember {
	font-family: Arial
}



.txt-box {
	position: relative;
	margin: 0 0 25px
}

.txt-box .tips {
	position: absolute;
	color: #666;
	top: 2px;
	left: 40px;
	display: inline-block;
	line-height: 16px;
	padding: 10px 6px 10px 0;
	cursor: text
}

.txt-box .txt {
	width: 274px;
	height: 38px;
	line-height: 38px\9;
	padding-left: 40px;
	border-radius: 2px;
	border: 1px solid #e5e5e5
}

.txt-box em {
	position: absolute;
	left: 10px;
	top: 8px;
	color: #ccc;
	font-size: 18px
}

.txt-box .error {
	border: 1px solid #f03468;
	box-shadow: 0 0 4px -2px #fa4678
}



.txt-btn {
	margin: 25px 0 0
}
.txt-btn a:hover {
	color: #f03468;
}
.ui-tiptext-error {
	position: absolute;
	left: 0;
	top: 42px;
	color: #f03468;
	width: 400px
}

.txt-box-pw .txt-icon {
	background-position: 0 -209px
}

.txt-box-pw .pw-stg-box {
	display: none;
	height: 14px;
	line-height: 14px;
	position: absolute;
	top: 10px;
	left: 325px;
	width: 230px
}

.txt-box-pw .pw-stg-box span {
	color: #999
}

.txt-box-pw .pw-stg {
	padding-left: 4px
}

.txt-box-pw .pw-stg li {
	float: left;
	margin-right: 1px;
	width: 55px;
	height: 14px;
	line-height: 16px;
	text-align: center;
	color: #fff;
	background: #ccc;
	padding-bottom: 3px
}

.txt-box-pw .pw-stg-weak .stg-level-1 {
	background: #ec9d21;
	color: #fff
}

.txt-box-pw .pw-stg-good {
	color: #fff
}

.txt-box-pw .pw-stg-good .stg-level-1 {
	background: #ffb525
}

.txt-box-pw .pw-stg-good .stg-level-2 {
	background: #ff630e
}

.txt-box-pw .pw-stg-strong {
	color: #fff
}

.txt-box-pw .pw-stg-strong .stg-level-1 {
	background: #ffb525
}

.txt-box-pw .pw-stg-strong .stg-level-2 {
	background: #ff630e
}

.txt-box-pw .pw-stg-strong .stg-level-3 {
	background: #f03468
}

.head {
	height: 90px
}

.middle,
.middle .w990 #jAd {
	height: 480px
}

.head .login .hd {
	margin-top: 23px;
}

.head .login .bd {
	color: #666;
	font-size: 22px;
	margin: 33px 0 0 8px;
	border-left: solid #e4e4e4 1px;
	padding-left: 8px
}

.head .login .ft {
	margin-top: 53px
}



.head .login .ft label {
	color: #999
}

.head .login .ft a:hover {
	color: #f03468
}

.footer {
	text-align: center;
	padding: 35px 0 5px;
	color: #999
}
.forget{
	float:right;
}
.footer a {
	color: #999;
	margin: 0 5px
}

.footer p {
	padding-top: 10px
}

.middle {
	background: 50% 50% no-repeat
}

.middle .w990 {
	position: relative
}

.middle .w990 #jAd a {
	display: block;
	height: 100%
}

.middle .login-panel {
	position: absolute;
	background: #fff;
	width: 350px;
	right: 0;
	top: 55px
}

.middle .login-panel .hd {
	position: relative;
	font-size: 16px;
	height: 53px;
	color: #999;
	border-bottom: solid #e4e4e4 1px
}

.middle .login-panel .hd span {
	float: left;
	width: 50%;
	text-align: center;
	margin-top: 20px;
	cursor: pointer
}

.middle .login-panel .hd .hover {
	color: #333
}

.middle .login-panel .hd i {
	position: absolute;
	color: #fff5f5;
	bottom: -1px;
	font-size: 20px;
	left: 46px;
	width: 80px;
	background: darkslateblue;
	height: 2px;
	overflow: hidden
}

.middle .login-panel .bd {
	overflow: hidden;
	position: relative
}

.middle .login-panel .bd ul {
	width: 700px
}

.middle .login-panel .bd li {
	float: left;
	width: 350px
}

.middle .login-panel .bd li form {
	width: 316px;
	margin: 0 auto
}

.middle .login-panel .bd li form .txt-box-margin {
	margin-top: 25px
}

.middle .login-panel .bd li form .txt {
	padding-left: 50px;
	width: 264px
}


.middle .login-panel .bd li form .btn {
	width: 316px
}

.middle .login-panel .bd li form .txt-btn {
	margin-bottom: 40px
}

.middle .login-panel .bd li form .txt-btn-height {
	margin-top: 15px
}
.err_msg{
	
	color: red;
}

.middle .login-panel .bd li form em {
	background: darkslateblue;
	left: 1px;
	top: 1px;
	width: 40px;
	color: #fff;
	text-align: center;
	line-height: 38px;
	font-size: 22px;
	-webkit-text-stroke-width: 0;
	height: 38px
}



